<div class="subhead">
  <h4>All Users</h4>
</div>
<div class="users">
  <ul>
    <% @users.each do |user| %>
      <li>
        <%= user.first_name %> <%= user.last_name %> | <%= link_to 'Delete User', user, method: :delete, data: { confirm: "Are you sure?" } %>
      </li>
    <% end %>
  </ul>
</div>

<div class="edit">
  <%= link_to 'Home', colors_path %>
</div>
